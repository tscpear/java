<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chapter6.mapper.UrlMapper">
    <select id="getUrl" resultType="com.chapter6.model.Url">
        select *
        from url
        where id = #{urlId};
    </select>
</mapper>